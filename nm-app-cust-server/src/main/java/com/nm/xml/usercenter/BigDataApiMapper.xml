<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nm.mapper.usercenter.BigDataApiMapper">
    <update id="isAuthFlag">
        UPDATE app_user_info set AUTH_FLAG=#{authFlag} where MOBL_NO=#{phone}
    </update>


    <delete id="deleteDmOrderInfo">
      delete from Dm_order_info where loan_no = #{loanNo}
    </delete>
</mapper>