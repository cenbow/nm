<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nm.mapper.usercenter.VersionToolMapper" >

  	<select id="queryVersionDetail" resultType="map">
		<![CDATA[
			SELECT t.*
			FROM app_version_updt_reg t
			WHERE t.APPTYPE = #{appType}
			]]>
	</select>
  
  
</mapper>